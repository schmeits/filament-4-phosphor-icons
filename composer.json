{
    "name": "schmeits/filament-4-phosphor-icons",
    "description": "This is my package filament-4-phosphor-icons",
    "keywords": [
        "schmeits",
        "laravel",
        "filament-4-phosphor-icons"
    ],
    "homepage": "https://github.com/schmeits/filament-4-phosphor-icons",
    "support": {
        "issues": "https://github.com/schmeits/filament-4-phosphor-icons/issues",
        "source": "https://github.com/schmeits/filament-4-phosphor-icons"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tally Schmeits",
            "email": "tally@schmeits.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "codeat3/blade-phosphor-icons": "^2.3",
        "filament/filament": "^v4.0.0-alpha6",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^3.2",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Schmeits\\FilamentPhosphorIcons\\": "src/",
            "Schmeits\\FilamentPhosphorIcons\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Schmeits\\FilamentPhosphorIcons\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Schmeits\\FilamentPhosphorIcons\\FilamentPhosphorIconsServiceProvider"
            ],
            "aliases": {
                "FilamentPhosphorIcons": "Schmeits\\FilamentPhosphorIcons\\Facades\\FilamentPhosphorIcons"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
